{
  "name": "LASI",
  "author": "lasi-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run concurrently \"npm run serve\" \"npm run bundle-flow\"",
    "serve": "lite-server",
    "test": "jspm run test",
    "postinstall": "jspm i && npm run ati",
    "ati": "ati i --explicitIndex false",
    "tsc": "tsc",
    "tslint": "tslint",
    "concurrently": "concurrently",
    "bundle-flow": "jspm unbundle && jspm bundle --source-map-contents -i plugin-typescript + plugin-babel + text + sass transpilers.bundle.js && jspm bundle -i app/** - [app/**] + aurelia-bootstrapper + aurelia-dialog + aurelia-pal + aurelia-history-browser + aurelia-templating + aurelia-templating-router + aurelia-templating-resources + aurelia-loader-default + aurelia-logging-console + aurelia-templating-binding deps.bundle.js && jspm bundle -wid --source-map-contents [app/**] app.bundle.js",
    "lint": "tslint --force --format stylish -c tslint.json --project tsconfig.json -e \"**/__test__/**\"",
    "bundle-dev": "jspm unbundle && jspm bundle app/** -wid",
    "bundle-prod": "jspm unbundle && jspm bundle app/** --minify",
    "build-dev": "jspm unbundle && jspm build app/** --format umd",
    "build-prod": "jspm unbundle && jspm build app/** --minify --format umd",
    "ci": "npm run lint && npm run test && npm run build-prod"
  },
  "jspm": {
    "main": "main.ts",
    "dependencies": {
      "@types/bootstrap": "npm:@types/bootstrap@^3.3.32",
      "@types/deep-freeze": "npm:@types/deep-freeze@^0.1.1",
      "@types/jquery": "npm:@types/jquery@latest",
      "@types/jquery-contextmenu": "npm:@types/jquery.contextmenu@^1.7.31",
      "@types/typeahead": "npm:@types/typeahead@^0.11.28",
      "aurelia-dialog": "npm:aurelia-dialog@^1.0.0-beta.3.0.1",
      "aurelia-http-client": "npm:aurelia-http-client@^1.0.0",
      "babel": "npm:babel-core@^6.21.0",
      "bootstrap": "github:twbs/bootstrap@^3.3.7",
      "deep-freeze": "npm:deep-freeze@^0.0.1",
      "flat-map": "github:aluanhaddad/flat-map@master",
      "font-awesome": "npm:font-awesome@^4.7.0",
      "jquery-contextmenu": "npm:jquery-contextmenu@^2.2.0",
      "reflect-metadata": "npm:reflect-metadata@^0.1.8",
      "rxjs": "npm:rxjs@^5.2.0",
      "sass": "github:dougludlow/plugin-sass@^0.6.0",
      "tslib": "npm:tslib@^1.6.1",
      "typeahead": "github:twitter/typeahead.js@^0.11.1"
    },
    "devDependencies": {
      "@types/blue-tape": "npm:@types/blue-tape@latest",
      "@types/tape": "npm:@types/tape@latest",
      "aurelia-testing": "npm:aurelia-testing@^1.0.0-beta.2.0.0",
      "bcrypt-pbkdf": "npm:bcrypt-pbkdf@^1.0.0",
      "css": "github:systemjs/plugin-css@^0.1.33",
      "debug": "npm:debug@^2.3.2",
      "ecc-jsbn": "npm:ecc-jsbn@~0.1.1",
      "fsevents": "npm:fsevents@^1.0.0",
      "jodid25519": "npm:jodid25519@^1.0.0",
      "jsbn": "npm:jsbn@0.1",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.25",
      "plugin-typescript": "github:frankwallis/plugin-typescript@^7.0.6",
      "request": "npm:request@^2.34",
      "source-map-support": "npm:source-map-support@^0.4.3",
      "tape": "npm:tape@^4.6.2",
      "text": "github:systemjs/plugin-text@^0.0.9",
      "timers": "npm:jspm-nodelibs-timers@^0.2.0",
      "tweetnacl": "npm:tweetnacl@0.14"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "aurelia-binding": "npm:aurelia-binding@^1.2.1",
      "aurelia-bootstrapper": "npm:aurelia-bootstrapper@^2.1.1",
      "aurelia-dependency-injection": "npm:aurelia-dependency-injection@^1.3.1",
      "aurelia-event-aggregator": "npm:aurelia-event-aggregator@^1.0.0",
      "aurelia-fetch-client": "npm:aurelia-fetch-client@^1.0.1",
      "aurelia-framework": "npm:aurelia-framework@^1.1.5",
      "aurelia-history": "npm:aurelia-history@^1.0.0",
      "aurelia-history-browser": "npm:aurelia-history-browser@^1.0.0",
      "aurelia-loader": "npm:aurelia-loader@^1.0.0",
      "aurelia-loader-default": "npm:aurelia-loader-default@^1.0.0",
      "aurelia-logging": "npm:aurelia-logging@^1.3.1",
      "aurelia-logging-console": "npm:aurelia-logging-console@^1.0.0",
      "aurelia-metadata": "npm:aurelia-metadata@^1.0.0",
      "aurelia-pal": "npm:aurelia-pal@^1.3.0",
      "aurelia-pal-browser": "npm:aurelia-pal-browser@^1.0.0",
      "aurelia-path": "npm:aurelia-path@^1.0.0",
      "aurelia-polyfills": "npm:aurelia-polyfills@^1.0.0",
      "aurelia-route-recognizer": "npm:aurelia-route-recognizer@^1.0.0",
      "aurelia-router": "npm:aurelia-router@^1.0.1",
      "aurelia-task-queue": "npm:aurelia-task-queue@^1.2.0",
      "aurelia-templating": "npm:aurelia-templating@^1.4.2",
      "aurelia-templating-binding": "npm:aurelia-templating-binding@^1.0.0",
      "aurelia-templating-resources": "npm:aurelia-templating-resources@^1.0.0",
      "aurelia-templating-router": "npm:aurelia-templating-router@^1.0.0",
      "aurelia-ui-virtualization": "npm:aurelia-ui-virtualization@^1.0.0-beta.3.0.2",
      "aurelia-validation": "npm:aurelia-validation@^1.0.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "core-js": "npm:core-js@^2.0.3",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "dgram": "npm:jspm-nodelibs-dgram@^0.2.0",
      "dns": "npm:jspm-nodelibs-dns@^0.2.0",
      "domain": "npm:jspm-nodelibs-domain@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "graceful-fs": "npm:graceful-fs@^4.1.6",
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "https": "npm:jspm-nodelibs-https@^0.2.0",
      "jquery": "npm:jquery@2",
      "kramed": "npm:kramed@^0.5.6",
      "lodash": "npm:lodash@^4.17.2",
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "moment": "npm:moment@^2.17.1",
      "net": "npm:jspm-nodelibs-net@^0.2.0",
      "numeral": "npm:numeral@^1.5.6",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "punycode": "npm:jspm-nodelibs-punycode@^0.2.0",
      "querystring": "npm:jspm-nodelibs-querystring@^0.2.0",
      "readline": "npm:jspm-nodelibs-readline@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "tls": "npm:jspm-nodelibs-tls@^0.2.0",
      "tty": "npm:jspm-nodelibs-tty@^0.2.0",
      "typescript": "npm:typescript@next",
      "url": "npm:jspm-nodelibs-url@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "whatwg-fetch": "npm:whatwg-fetch@^2.0.1",
      "zlib": "npm:jspm-nodelibs-zlib@^0.2.0"
    },
    "overrides": {
      "github:frankwallis/plugin-typescript@7.1.1": {
        "peerDependencies": {
          "typescript": "next"
        }
      },
      "github:twbs/bootstrap@3.3.7": {
        "shim": {
          "js/bootstrap": {
            "deps": [
              "jquery"
            ],
            "exports": "$"
          }
        },
        "dependencies": {
          "jquery": "*"
        },
        "files": [
          "dist",
          "fonts",
          "js",
          "css",
          "less",
          "grunt",
          "LICENSE"
        ],
        "systemjs": {
          "main": "dist/js/bootstrap.js",
          "modules": {
            "dist/js/bootstrap.js": {
              "deps": [
                "jquery"
              ],
              "exports": "$"
            }
          }
        }
      },
      "github:twitter/typeahead.js@0.11.1": {
        "dependencies": {
          "jquery": "*"
        },
        "meta": {
          "dist/typeahead.bundle.js": {
            "deps": [
              "jquery"
            ],
            "format": "global"
          }
        }
      },
      "npm:@types/bootstrap@3.3.36": {
        "dependencies": {}
      },
      "npm:aurelia-dialog@1.0.0-rc.2.0.0": {
        "main": "dist/es2015/index",
        "format": "esm",
        "directories": {
          "lib": ""
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.6.9": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:font-awesome@4.7.0": {
        "main": "css/font-awesome.css!",
        "jspmNodeConversion": false,
        "registry": "jspm",
        "dependencies": {
          "css": "0.1"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@2.2.4": {
        "format": "amd"
      },
      "npm:lodash@4.17.4": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:moment@2.19.2": {
        "jspmNodeConversion": false
      },
      "npm:pbkdf2@3.0.14": {
        "main": "browser.js"
      },
      "npm:reflect-metadata@0.1.10": {
        "jspmNodeConversion": false,
        "map": {
          "crypto": {
            "node": "@node/crypto",
            "default": "@empty"
          }
        }
      },
      "npm:safe-buffer@5.1.1": {
        "browser": "index.js"
      },
      "npm:sass.js@0.9.13": {
        "map": {
          "ws": "@empty"
        },
        "meta": {
          "dist/sass.worker.js": {
            "format": "global"
          }
        }
      },
      "npm:tslib@1.8.0": {
        "main": "tslib.es6.js",
        "format": "esm",
        "meta": {
          "tslib.js": {
            "esModule": true
          }
        }
      },
      "npm:typescript@next": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        },
        "dependencies": {
          "source-map-support": "*"
        }
      }
    }
  },
  "devDependencies": {
    "aurelia-types-installer": "^0.4.0",
    "concurrently": "^3.5.0",
    "jspm": "beta",
    "lite-server": "^2.3.0",
    "tslint": "latest",
    "typescript": "next"
  },
  "dependencies": {}
}
